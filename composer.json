{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "laravel-lang/lang": "^15.26",
        "laravel/framework": "^12.0",
        "laravel/sanctum": "^4.2",
        "laravel/tinker": "^2.10.1",
        "nwidart/laravel-modules": "^12.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.24",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "phpunit/phpunit": "^11.5.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\Run\\": "Modules/Run/app/",
            "Modules\\Users\\": "Modules/Users/app/",
            "Modules\\Locations\\": "Modules/Locations/app/",
            "Modules\\Addresses\\": "Modules/Addresses/app/",
            "Modules\\Wallet\\": "Modules/Wallet/app/",
            "Modules\\Sliders\\": "Modules/Sliders/app/",
            "Modules\\Menus\\": "Modules/Menus/app/",
            "Modules\\Wishlist\\": "Modules/Wishlist/app/",
            "Modules\\Cart\\": "Modules/Cart/app/",
            "Modules\\Contact\\": "Modules/Contact/app/",
            "Modules\\Products\\": "Modules/Products/app/",
            "Modules\\Gateway\\": "Modules/Gateway/app/",
            "Modules\\Notifications\\": "Modules/Notifications/app/",
            "Modules\\Specifications\\": "Modules/Specifications/app/",
            "Modules\\Reports\\": "Modules/Reports/app/",
            "Modules\\Dashboard\\": "Modules/Dashboard/app/",
            "Modules\\Categories\\": "Modules/Categories/app/",
            "Modules\\Attributes\\": "Modules/Attributes/app/",
            "Modules\\Articles\\": "Modules/Articles/app/",
            "Modules\\Front\\": "Modules/Front/app/",
            "Modules\\ArticleCategories\\": "Modules/ArticleCategories/app/",
            "Modules\\Coupons\\": "Modules/Coupons/app/",
            "Modules\\Banners\\": "Modules/Banners/app/",
            "Modules\\Comments\\": "Modules/Comments/app/",
            "Modules\\Shipping\\": "Modules/Shipping/app/",
            "Modules\\Orders\\": "Modules/Orders/app/",
            "Modules\\Settings\\": "Modules/Settings/app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite --kill-others"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "wikimedia/composer-merge-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
